<script type="text/javascript">

  //$('#category_id').val({{category_var}}).change();

 $(".form_datetime").datetimepicker({
	format: "yyyy-mm-dd",
	minView: 'month',
	autoclose: true
	 
});



function filter_advert() {
	
	startDate_var = $('#startDate').val()
	endDate_var = $('#endDate').val()
	category_name_var = $('#category_id').val()
	stat_var = $("input[name='stat']:checked").val()

	location.href = '/subscriber-advert/?supplier_id=110&start_date_var='+startDate_var+'&end_date_var='+endDate_var+'&status_var='+stat_var+'&category_var	='+category_name_var+''
}


function activate_daterange() {

   $('input[type="button"]').click(function() {

       if($(this).attr('id') == 'One_week') {            
            
            var date = new Date();

           //var date = new Date($('#start_'+id).val());
		        // days = parseInt($('#day_'+id).val(), 10) - 1;
		        date.setDate(date.getDate() - 7 );
		        var yyyy = date.getFullYear().toString();
		        var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
		        var dd  = date.getDate().toString();
		        newdate = yyyy + "-"+  (mm[1]?mm:"0"+mm[0]) + "-" +  (dd[1]?dd:"0"+dd[0]);
        
				$("#startDate").val(newdate);
          
           
            var date = new Date();

		        var yyyy = date.getFullYear().toString();
		        var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
		        var dd  = date.getDate().toString();
		         newdate = yyyy + "-"+  (mm[1]?mm:"0"+mm[0]) + "-" +  (dd[1]?dd:"0"+dd[0]);
        
				$("#endDate").val(newdate);

    }
    else if($(this).attr('id') == 'One_month'){
                       var date = new Date();

		        date.setDate(date.getDate() - 30 );
		        var yyyy = date.getFullYear().toString();
		        var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
		        var dd  = date.getDate().toString();
		          newdate = yyyy + "-"+  (mm[1]?mm:"0"+mm[0]) + "-" +  (dd[1]?dd:"0"+dd[0]);
        
				$("#startDate").val(newdate);
          
           
            var date = new Date();

		        var yyyy = date.getFullYear().toString();
		        var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
		        var dd  = date.getDate().toString();
		         newdate = yyyy + "-"+  (mm[1]?mm:"0"+mm[0]) + "-" +  (dd[1]?dd:"0"+dd[0]);
        
				$("#endDate").val(newdate);
       }
           else if($(this).attr('id') == 'Three_month'){
                       var date = new Date();

		        date.setDate(date.getDate() - 90 );
		        var yyyy = date.getFullYear().toString();
		        var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
		        var dd  = date.getDate().toString();
		          newdate = yyyy + "-"+  (mm[1]?mm:"0"+mm[0]) + "-" +  (dd[1]?dd:"0"+dd[0]);
        
				$("#startDate").val(newdate);
          
           
            var date = new Date();

		        var yyyy = date.getFullYear().toString();
		        var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
		        var dd  = date.getDate().toString();
		           newdate = yyyy + "-"+  (mm[1]?mm:"0"+mm[0]) + "-" +  (dd[1]?dd:"0"+dd[0]);
        
				$("#endDate").val(newdate);
       }
           else if($(this).attr('id') == 'Six_month'){
                       var date = new Date();

		        date.setDate(date.getDate() - 180 );
		        var yyyy = date.getFullYear().toString();
		        var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
		        var dd  = date.getDate().toString();
		         newdate = yyyy + "-"+  (mm[1]?mm:"0"+mm[0]) + "-" +  (dd[1]?dd:"0"+dd[0]);
        
				$("#startDate").val(newdate);
          
           
            var date = new Date();

		        var yyyy = date.getFullYear().toString();
		        var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
		        var dd  = date.getDate().toString();
		         newdate = yyyy + "-"+  (mm[1]?mm:"0"+mm[0]) + "-" +  (dd[1]?dd:"0"+dd[0]);
        
				$("#endDate").val(newdate);
       }
                  else if($(this).attr('id') == 'date_range'){
        
				$("#startDate").val(null);
        
				$("#endDate").val(null);
       }
 
   });
}
</script>        
