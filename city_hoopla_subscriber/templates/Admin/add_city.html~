    ctx_c.fillText("Image Preview",90,80);

    function remove_modal(place_type,div_id){
        id = parseInt(div_id) - 1;
        remove_text = "Are you sure you would like to delete this "
        if(place_type == "1"){
            if(document.getElementById("poi"+id).files[0] == undefined && $("#about_poi_"+div_id).val().trim() == ''){
                $("#poi_div_"+div_id).remove();
            }
            else{
                remove_text = remove_text + "city attraction?"
                $("#remove-modal").modal('show');
            }

        }
        if(place_type == "2"){
            if(document.getElementById("shop"+id).files[0] == undefined && $("#about_shop_"+div_id).val().trim() == ''){
                $("#shop_div_"+div_id).remove();
            }
            else{
                remove_text = remove_text + "shopping?"
                $("#remove-modal").modal('show');
            }
        }
        if(place_type == "3"){
            if(document.getElementById("hospital"+id).files[0] == undefined && $("#about_hos_"+div_id).val().trim() == ''){
                $("#hospital_div_"+div_id).remove();
            }
            else{
                remove_text = remove_text + "hospital?"
                $("#remove-modal").modal('show');
            }
        }
        if(place_type == "4"){
            if(document.getElementById("college"+id).files[0] == undefined && $("#about_clg_"+div_id).val().trim() == ''){
                $("#college_div_"+div_id).remove();
            }
            else{
                remove_text = remove_text + "education?"
                $("#remove-modal").modal('show');
            }
        }
        $("#remove_text").text(remove_text);
        $("#remove_div_id").val(div_id);
        $("#remove_div_type").val(place_type);
    }

    function remove_div(){
        div_id = $("#remove_div_id").val();
        place_type = $("#remove_div_type").val();
        if(place_type == "1"){
            $("#poi_div_"+div_id).remove();
        }
        if(place_type == "2"){
            $("#shop_div_"+div_id).remove();
        }
        if(place_type == "3"){
            $("#hospital_div_"+div_id).remove();
        }
        if(place_type == "4"){
            $("#college_div_"+div_id).remove();
        }
        $("#remove-modal").modal('hide');
    }

    var poi_index = 1;
    var poi_div_index = 2;

    $(".add_point_of_interest").click(function(){
