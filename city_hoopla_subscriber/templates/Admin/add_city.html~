            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
</div>

<div class="modal fade draggable-modal" id="remove-modal" tabindex="-1" role="basic" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">

                <img style="width: 100px; height: 29px; margin-top: -11px; margin-left: 236px;"
                     src="/static/assets/layouts/layout2/img/City_Hoopla_Logo.png" alt="logo" class="logo-default">

            </div>
            <div class="modal-body">

                <div class="form-body">
                    <h4 class="text-center" id="remove_text"></h4>
                    <input id="remove_div_id" hidden>
                    <input id="remove_div_type" hidden>
                </div>
                <div style="text-align:center !important;" class="modal-footer">
                    <button type="button" class="btn green" onclick="remove_div()" >Yes</button>
                    <button type="button" class="btn green" data-dismiss="modal">No</button>

                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
</div>

