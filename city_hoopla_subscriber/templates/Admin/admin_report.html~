}
function tab_change4(){
$("#subscriber").css("display","none");
$("#sales").css("display","none");
$("#management").css("display","none");
$("#analytical").css("display","block");

 table_init17();
}


function mngt_change(){

if($("#mngt_clist").val() == "1"){
  $("#Register").css("display","block");
  $("#Activity").css("display","none");
  $("#Usage").css("display","none");
  $("#Database").css("display","none");
  $("#Sales_report").css("display","none");
  $("#new_sub_report").css("display","none");
  $("#new_subscrip_report").css("display","none");
  $("#payment_report").css("display","none");
  $("#advert_status").css("display","none");
  table_init9();
}
else if ($("#mngt_clist").val() == "2") {
  $("#Register").css("display","none");
  $("#Activity").css("display","block");
  $("#Usage").css("display","none");
  $("#Database").css("display","none");
  $("#Sales_report").css("display","none");
  $("#new_sub_report").css("display","none");
  $("#new_subscrip_report").css("display","none");
  $("#payment_report").css("display","none");
  $("#advert_status").css("display","none");
  table_init10();
}
else if ($("#mngt_clist").val() == "3") {
  $("#Register").css("display","none");
  $("#Activity").css("display","none");
  $("#Usage").css("display","block");
  $("#Database").css("display","none");
  $("#Sales_report").css("display","none");
  $("#new_sub_report").css("display","none");
  $("#new_subscrip_report").css("display","none");
  $("#payment_report").css("display","none");
  $("#advert_status").css("display","none");
  table_init11();
}
else if ($("#mngt_clist").val() == "4") {
  $("#Register").css("display","none");
  $("#Activity").css("display","none");
  $("#Usage").css("display","none");
  $("#Database").css("display","block");
  $("#Sales_report").css("display","none");
  $("#new_sub_report").css("display","none");
  $("#new_subscrip_report").css("display","none");
  $("#payment_report").css("display","none");
  $("#advert_status").css("display","none");
  advert_city_change();
}
else if ($("#mngt_clist").val() == "5") {
  $("#Register").css("display","none");
  $("#Activity").css("display","none");
  $("#Usage").css("display","none");
  $("#Database").css("display","none");
  $("#Sales_report").css("display","block");
  $("#new_sub_report").css("display","none");
  $("#new_subscrip_report").css("display","none");
  $("#payment_report").css("display","none");
  $("#advert_status").css("display","none");
  table_init13();
}
else if ($("#mngt_clist").val() == "6") {
  $("#Register").css("display","none");
  $("#Activity").css("display","none");
  $("#Usage").css("display","none");
  $("#Database").css("display","none");
  $("#Sales_report").css("display","none");
  $("#new_sub_report").css("display","block");
  $("#new_subscrip_report").css("display","none");
  $("#payment_report").css("display","none");
  $("#advert_status").css("display","none");
  table_init14();

}
else if ($("#mngt_clist").val() == "7") {
  $("#Register").css("display","none");
  $("#Activity").css("display","none");
  $("#Usage").css("display","none");
  $("#Database").css("display","none");
  $("#Sales_report").css("display","none");
  $("#new_sub_report").css("display","none");
  $("#new_subscrip_report").css("display","block");
  $("#payment_report").css("display","none");
  $("#advert_status").css("display","none");
  table_init15();
}
else if ($("#mngt_clist").val() == "8") {
  $("#Register").css("display","none");
  $("#Activity").css("display","none");
  $("#Usage").css("display","none");
  $("#Database").css("display","none");
  $("#Sales_report").css("display","none");
  $("#new_sub_report").css("display","none");
  $("#new_subscrip_report").css("display","none");
  $("#payment_report").css("display","block");
  $("#advert_status").css("display","none");
  table_init16();
}
else if ($("#mngt_clist").val() == "9") {
  $("#Register").css("display","none");
  $("#Activity").css("display","none");
  $("#Usage").css("display","none");
  $("#Database").css("display","none");
  $("#Sales_report").css("display","none");
  $("#new_sub_report").css("display","none");
  $("#new_subscrip_report").css("display","none");
  $("#payment_report").css("display","none");
  $("#advert_status").css("display","block");
 // table_init17();
}

}

$( document ).ready(function() {
    $(".sel2").select2({
        width: '100%'
    })
});


    $(".date-picker").datepicker({maxDate: '0', autoclose: true}).on('changeDate', function(e){$(this).datepicker('hide');});;

    table_init();
    table_init2();
    table_init3();
    table_init4();
    table_init9();
    table_init10();
    table_init11();
    table_init12();
     table_init13();
     table_init14();
     table_init15();
     table_init16();
     advert_city_change();
     table_init17();

    var doc = {
        pageSize: config.pageSize,
        pageOrientation: config.orientation,
        content: [
            {
                table: {
                    headerRows: 1,
                    body: rows
                },
                layout: 'noBorders'
            }
        ],
        styles: {
            tableHeader: {
                bold: true,
                fontSize: 11,
                color: 'white',
                fillColor: '#2d4154',
                alignment: 'center'
            },
            tableBodyEven: {},
            tableBodyOdd: {
                fillColor: '#f3f3f3'
            },
            tableFooter: {
                bold: true,
                fontSize: 11,
                color: 'white',
                fillColor: '#2d4154'
            },
            title: {
                alignment: 'center',
                fontSize: 15
            },
            message: {}
        },
        defaultStyle: {
            fontSize: 10
        }
    };



    function table_init(){
        $('#advert_table').removeAttr('width').dataTable({
            "bDestroy" : true,
            buttons: [

                { extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                    filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report",
                    customize: function(doc) {
                        doc.defaultStyle.fontSize = 8; //<-- set fontsize to 16 instead of 10
                        doc.defaultStyle.alignment= 'center'; //<-- set fontsize to 16 instead of 10
                        doc.styles.tableHeader.fontSize = 8; //<-- set fontsize to 16 instead of 10
                    }
                },
               //    { extend: 'excel', className: 'btn yellow btn-outline ',
               //     filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report"
                //},
            ],

            "order": [
                [0, 'asc']
            ],

            "ajax": "/get_advert_health/?advert_id=&from_date=&to_date=",
            "columns": [
                {"data": "advert_id","sClass": "text-center"},
                {"data": "advert_title"},
                {"data": "advert_views","sClass": "text-center"},
                {"data": "advert_likes","sClass": "text-center"},
                {"data": "advert_favourites","sClass": "text-center"},
                {"data": "advert_calls","sClass": "text-center"},
                {"data": "advert_call_backs","sClass": "text-center"},
                {"data": "advert_emails","sClass": "text-center"},
                {"data": "advert_coupons","sClass": "text-center"},
                {"data": "advert_reviews","sClass": "text-center"},
                {"data": "advert_sms","sClass": "text-center"},
                {"data": "advert_whatsapp","sClass": "text-center"},
                {"data": "advert_facebook","sClass": "text-center"},
                {"data": "advert_twitter","sClass": "text-center"}
            ],

               "columnDefs": [
                {"className": "dt-center", "targets": "_all", width: 150}
            ],

             "paging":   true,

             "scrollX":     true,
                 "filter": false,
             "info":     false,
          // .removeAttr('width').
            "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable

        });



    }

