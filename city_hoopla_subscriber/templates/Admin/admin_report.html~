    function mngt_consumer_change(){

        table_init9();
    }


    function table_init9(){
        var city_name = "All";
        var report_start_date ='';
        var report_end_date ='';
        if($("#mngt_city_list").val() != "" ){
            city_name = $("#mngt_city_list :selected").text().trim();
        }
        report_start_date = $("#consumer_from_date").val();
        report_end_date = $("#consumer_to_date").val();
        dates = report_start_date + " to " + report_end_date
        mngt_city_id = $("#mngt_city_list").val();
        from_date = $("#consumer_from_date").val();
        to_date = $("#consumer_to_date").val();
        total_guest_user = "0";
        total_register_user = "0";

        $.ajax({
            type : "GET",
            url : '/get-registered-consumer-data/',
            data : {
                'mngt_city':mngt_city_id,
                'from_date':from_date,
                'to_date':to_date,
            },
            success: function (response) {
                total_guest_user = response.total_guest_user;
                total_register_user = response.total_register_user;
            },
            beforeSend: function () {
                $("#processing").css('display','block');
            },
            complete: function () {
                $("#processing").css('display','none');
                $('#mgmt_consumers').removeAttr('width').dataTable({
                    "bDestroy" : true,
                    buttons: [
                        {
                            extend: 'excel', className: 'btn yellow btn-outline ',
                            filename: 'Registered Consumers Report', "title": "CityHoopla : Registered Consumers Report"
                        },
                        {
                            extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                            filename: 'Registered Consumers Report', "title": "CityHoopla :  Registered Consumers Report",
                            message: '\nCity: '+city_name+' \n\nDate Range: '+dates+' \n\nTotal Guest User(s): '+total_guest_user+' \n\nTotal Registered User(s): '+total_register_user+'\n\n',
                            customize: function(doc) {
                                doc.defaultStyle.fontSize = 16;
                                //doc.defaultStyle.alignment= 'center';
                                doc.styles.tableHeader.fontSize = 16;
                            }
                        },
                    ],

                    "order": [
                        [0, 'asc']
                    ],

                    "ajax": "/get_new_registered_consumer/?from_date="+from_date+"&to_date="+to_date+"&mngt_city="+mngt_city_id,
                    "columns": [
                         {"data": "sr_no","sClass": "text-center"},
                        {"data": "user_name"},
                        {"data": "date","sClass": "text-center"},
                        {"data": "consumer_contact_no"},
                        // {"data": "city"}
                    ],

                      "columnDefs": [
                        {"className": "dt-center", "targets": "_all", width: 180}
                    ],

                     "paging":   true,

                     "scrollX":     true,
                         "filter": false,
                     "info":     false,
                  // removeAttr('width').
                    "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable

                });
            },
            error : function(response){
                alert("_Error");
            }
        });

    }

