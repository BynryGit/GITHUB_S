<script type="text/javascript">
$("#filter-status").click(function(event)  {
    if ($("#cat_list").val() != '') {
			
        $("#advert_status_div").css("display","block");
        city_list = $("#city_list1").val();
        from_date = $("#from_date_stat").val();
        end_date = $("#to_date_stat").val();
        cat_id = $("#cat_list").val();
        data = '/get_advert_status_data/?cat_id='+cat_id+'&from_date='+from_date+'&to_date='+end_date+'&city_list='+city_list
        $('#advert_status_table').removeAttr('width').dataTable({
            "bDestroy" : true,
            buttons: [
                //{ extend: 'print', className: 'btn dark btn-outline',
                //filename: 'Advert Expire Status Expiring Adverts', "title": "CityHoopla : Expiring Adverts" },
                 { extend: 'excel', className: 'btn yellow btn-outline ',
                  filename: 'Expiring Adverts', "title": "CityHoopla : Expiring Adverts"
                },
                { extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                    filename: 'Expiring Adverts', "title": "CityHoopla : Expiring Adverts	",
                    customize: function(doc) {
                        doc.defaultStyle.fontSize = 8; //<-- set fontsize to 16 instead of 10
                        doc.defaultStyle.alignment= 'center'; //<-- set fontsize to 16 instead of 10
                        doc.styles.tableHeader.fontSize = 8; //<-- set fontsize to 16 instead of 10
                    }
                },
            ],
            
        
            "order": [
                [0, 'asc']
            ],
            "ajax": data,
            "columns": [
                {"data": "sr_id","sClass": "text-center"},
                {"data": "supplier_id","sClass": "text-center"},
                {"data": "business_name"},
                {"data": "advert_id","sClass": "text-center"},
                {"data": "advert_name"},                 
                {"data": "category"},
                {"data": "start_date","sClass": "text-center"},
                {"data": "end_date","sClass": "text-center"},
                {"data": "updated_by"},
                {"data": "status","sClass": "text-center"}
        
            ],
            "columnDefs": [
                {"className": "dt-center", "targets": "_all", width: 100}
            ],
            
            "paging":   true,
            "scrollX":     true,
            "filter": false,
            "info":     false,
            "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable
        });
    }
});


$("#filter-continue").click(function(event)  {
    if ($("#advert_list").val() != '') {

        $("#advert_table_display").css("display","block");
        city_list = $("#city_list").val();
        subscriber_list = $("#subscriber_list").val();
        from_date = $("#from_date").val();
        end_date = $("#to_date").val();
        advert_id = $("#advert_list").val();
        data = '/get_filter_data/?advert_id='+advert_id+'&from_date='+from_date+'&to_date='+end_date+'&city_list='+city_list+'&subscriber_list='+subscriber_list
        $('#advert_table').removeAttr('width').dataTable({
            "bDestroy" : true,
            buttons: [
                //{ extend: 'print', className: 'btn dark btn-outline',
                //filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report" },
                { extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                    filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report",
                    customize: function(doc) {
                        doc.defaultStyle.fontSize = 8; //<-- set fontsize to 16 instead of 10
                        doc.defaultStyle.alignment= 'center'; //<-- set fontsize to 16 instead of 10
                        doc.styles.tableHeader.fontSize = 8; //<-- set fontsize to 16 instead of 10
                    }
                },
            ],
            "order": [
                [0, 'asc']
            ],
            "ajax": data,
            "columns": [
                {"data": "advert_id","sClass": "text-center"},
                {"data": "advert_title"},
                {"data": "category_data"},
                {"data": "advert_views","sClass": "text-center"},
                {"data": "advert_likes","sClass": "text-center"},
                {"data": "advert_favourites","sClass": "text-center"},
                {"data": "advert_calls","sClass": "text-center"},
                {"data": "advert_call_backs","sClass": "text-center"},
                {"data": "advert_emails","sClass": "text-center"},
                {"data": "advert_coupons","sClass": "text-center"},
                {"data": "advert_reviews","sClass": "text-center"},
                {"data": "advert_sms","sClass": "text-center"},
                {"data": "advert_whatsapp","sClass": "text-center"},
                {"data": "advert_facebook","sClass": "text-center"},
                {"data": "advert_twitter","sClass": "text-center"}
            ],
            "columnDefs": [
                {"className": "dt-center", "targets": "_all", width: 100}
            ],
            "paging":   true,
            "scrollX":     true,
            "filter": false,
            "info":     false,
            "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable
        });
    }
});

    $("#filter-continue1").click(function(event)  {
        $("#subscription_plan_display").css("display","block");
        city_list = $("#sub_city_list").val();
        subscriber_list = $("#subscribers_list").val();
        data = '/get_filter_data1/?city_list='+city_list+'&subscriber_list='+subscriber_list
        $('#subscription_plan').removeAttr('width').dataTable({
            "bDestroy" : true,
            buttons: [
                { extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                    filename: 'Subscription Plan', "title": "CityHoopla : Subscription Plan",
                    customize: function(doc) {
                        doc.defaultStyle.fontSize = 6; //<-- set fontsize to 16 instead of 10
                        doc.defaultStyle.alignment= 'center'; //<-- set fontsize to 16 instead of 10
                        doc.styles.tableHeader.fontSize = 6; //<-- set fontsize to 16 instead of 10
                    }
                },
            ],
            "order": [
                [0, 'asc']
            ],
            "ajax": data,
            "columns": [
                {"data": "advert_id","sClass": "text-center"},
                {"data": "advert_title"},
                {"data": "category"},
                {"data": "start_date","sClass": "text-center"},
                {"data": "end_date","sClass": "text-center"},
                {"data": "premium_service"},
                {"data": "premium_start_date","sClass": "text-center"},
                {"data": "premium_end_date","sClass": "text-center"},
                {"data": "advert_slider","sClass": "text-center"},
                {"data": "slider_start_date","sClass": "text-center"},
                {"data": "slider_end_date","sClass": "text-center"},
                {"data": "top_advert","sClass": "text-center"},
                {"data": "top_advert_start_date","sClass": "text-center"},
                {"data": "top_advert_end_date","sClass": "text-center"},
                {"data": "uploaded_pictures","sClass": "text-center"},
                {"data": "uploaded_videos","sClass": "text-center"},
                {"data": "memory_usages","sClass": "text-center"},
                {"data": "total_service_cost","sClass": "text-center"},
                {"data": "total_amount_paid","sClass": "text-center"},
                {"data": "saleman_name"},
                {"data": "saleman_number","sClass": "text-center"}
            ],
            "columnDefs": [
                {"className": "dt-center", "targets": "_all", width: 90}
            ],
            "paging": true,
            "scrollX": true,
            "filter": false,
            "info": false,
            "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable
        });
    });

