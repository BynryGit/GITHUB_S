function get_sales_staff(){
    city_id = $("#my_sub_city_list").val();
    $("#my_subscriber").css("display","none");
    $("#sales_subscriber_div").css("display","none");
    if(city_id){
        $.ajax({
            type: 'GET',
            url: '/get-sales-staff-list/',
            data:{'city_id':city_id},
            success: function (response) {
                $("#sales_staff_list").html("");
                $("#sales_staff_list").append("<option value=''>Select Sales Staff</option>");
                if (response.sales_staff_list.length > 0) {
                        $("#sales_staff_list").append("<option value='all'>All</option>");
                }
                $.each(response.sales_staff_list, function (index, item) {
                    data = '<option value="'+ item.user_id +'">'+ item.sales_person_name +'</option>'
                    $("#sales_staff_list").append(data);
                });
                $("#sales_staff_list").val("").change();
            },
            error: function (response) {
                alert("Error!");
            },
        });
    }
    else{
        $("#sales_staff_list").html("");
        $("#sales_staff_list").append("<option value=''>Select Sales Staff</option>");
        $("#sales_staff_list").val("").change();
    }
}

function get_sale_staff(){
    city_id = $("#sale_city_list").val();
    $("#my_subscriber_sale").css("display","none");
    $("#sale_subscriber_div").css("display","none");
    if(city_id){
        $.ajax({
            type: 'GET',
            url: '/get-sales-staff-list/',
            data:{'city_id':city_id},
            success: function (response) {
                $("#sale_staff_list").html("");
                $("#sale_staff_list").append("<option value=''>Select Sales Staff</option>");
                if (response.sales_staff_list.length > 0) {
                        $("#sale_staff_list").append("<option value='all'>All</option>");
                }
                $.each(response.sales_staff_list, function (index, item) {
                    data = '<option value="'+ item.user_id +'">'+ item.sales_person_name +'</option>'
                    $("#sale_staff_list").append(data);
                });
                $("#sale_staff_list").val("").change();
            },
            error: function (response) {
                alert("Error!");
            },
        });
    }
    else{
        $("#sale_staff_list").html("");
        $("#sale_staff_list").append("<option value=''>Select Sales Staff</option>");
        $("#sale_staff_list").val("").change();
    }
}

function show_my_subscriber(){
    if($("#sales_staff_list").val()){
        $("#my_subscriber").css("display","block");
    }
    else{
        $("#my_subscriber").css("display","none");
    }
}

function show_my_subscriber_sale(){
    if($("#sale_staff_list").val()){
        $("#my_subscriber_sale").css("display","block");
    }
    else{
        $("#my_subscriber_sale").css("display","none");
    }
}

function filter_my_subscriber(){
    sub_city_id = $("#my_sub_city_list").val();
    my_from_date = $("#my_from_date").val();
    my_to_date = $("#my_to_date").val();
    user_id = $("#sales_staff_list").val();
    $("#sales_subscriber_div").css("display","block");
    data = '/my_subscribers_list/?from_date='+my_from_date+'&to_date='+my_to_date+'&sub_city='+sub_city_id+'&user_id='+user_id;
    $('#sales_subscriber').dataTable({
        "bDestroy" : true,
        buttons: [
            { extend: 'excel', className: 'btn yellow btn-outline ',
                filename: 'My Subscriber', "title": "CityHoopla : My Subscriber"
            },
            { extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                filename: 'My Subscriber', "title": "CityHoopla : My Subscriber",
                customize: function(doc) {
                    doc.defaultStyle.fontSize = 15; //<-- set fontsize to 16 instead of 10
                    doc.defaultStyle.alignment= 'center'; //<-- set fontsize to 16 instead of 10
                    doc.styles.tableHeader.fontSize = 15; //<-- set fontsize to 16 instead of 10
                }
            },
        ],
        "order": [
            [0, 'asc']
        ],
        "ajax": data,
        "columns": [
            {"data": "subscriber_id","sClass": "text-center"},
            {"data": "business_name"},
            {"data": "poc_name"},
            {"data": "poc_no","sClass": "text-center"},
            {"data": "city"},
            {"data": "created_date","sClass": "text-center"},
            {"data": "total_amount","sClass": "text-center"},
            {"data": "sales_partner"},
            {"data": "status","sClass": "text-center"}
        ],
        "columnDefs": [
           {"className": "dt-center", "targets": "_all", width: 150}
        ],
        "paging":   true,
        "scrollX": true,
        "filter": false,
        "info":     false,
        "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable
    });
}

