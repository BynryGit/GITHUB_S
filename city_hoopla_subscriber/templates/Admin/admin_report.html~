function get_category1_data(){


        $("#cat_list").html("");
        $("#cat_list").append("<option value=''>Select Category</option>");
        $('#cat_list').val("").change();
        $("#cat_list1").html("");
        $("#cat_list1").append("<option value=''>Select Category Level 1</option>");
        $('#cat_list1').val("").change();
        $("#cat_list2").html("");
        $("#cat_list2").append("<option value=''>Select Category Level 2</option>");
        $('#cat_list2').val("").change();

        $("#cat_list3").html("");
        $("#cat_list3").append("<option value=''>Select Category Level 3</option>");
        $('#cat_list3').val("").change();

        $("#cat_list4").html("");
        $("#cat_list4").append("<option value=''>Select Category Level 4</option>");
        $('#cat_list4').val("").change();

        $("#cat_list5").html("");
        $("#cat_list5").append("<option value=''>Select Category Level 5</option>");
        $('#cat_list5').val("").change();

        $("#advert_list").html("");
        $("#advert_list").append("<option value=''>Select Advert</option>");
        $('#advert_list').val("").change();

          $('#sub_err').css("display", "none");
          $("#div1").css("display","none");
         $("#div2").css("display","none");
         $("#div3").css("display","none");
         $("#div4").css("display","none");
         $("#div5").css("display","none");
         $("#div6").css("display","none");
        

       if ($("#subscriber_list").val() == '') {
        return false;
       }


     $.ajax({
        type: 'GET',
        url: '/get-category1-list/',
        data:{'subscriber_id':$("#subscriber_list").val()},
        success: function (response) {

            $("#div1").css("display","block");
            $("#cat_list").html("");
            $("#cat_list").append("<option value=''>Select Category</option>");

            if (response.category_list.length > 0) {
                   $("#cat_list").append("<option value='all'>All</option>");
             }


            $.each(response.category_list, function (index, item) {
                data = '<option value="'+ item.category_id +'">'+ item.category_nm +'</option>'
                $("#cat_list").append(data);
            });

        },
        error: function (response) {
            alert("Error!");
        },

    });


}


function get_cat1_data(){
        $("#cat_list1").html("");
        $("#cat_list1").append("<option value=''>Select Category Level 1</option>");
        $('#cat_list1').val("").change();
        $("#cat_list2").html("");
        $("#cat_list2").append("<option value=''>Select Category Level 2</option>");
        $('#cat_list2').val("").change();

        $("#cat_list3").html("");
        $("#cat_list3").append("<option value=''>Select Category Level 3</option>");
        $('#cat_list3').val("").change();

        $("#cat_list4").html("");
        $("#cat_list4").append("<option value=''>Select Category Level 4</option>");
        $('#cat_list4').val("").change();

        $("#cat_list5").html("");
        $("#cat_list5").append("<option value=''>Select Category Level 5</option>");
        $('#cat_list5').val("").change();

         $('#sub_err').css("display", "none");
         $("#div22").css("display","none");
         $("#div33").css("display","none");
         $("#div44").css("display","none");
         $("#div55").css("display","none");
         $("#div66").css("display","none");
         $("#advert_status_filter").css("display","none");
         


    if($("#cat_list").val() == ""){
    	  $("#advert_status_filter").css("display","none");
        return false;
     }

    $.ajax({
        type: 'GET',
        url: '/get-catlevel1-list/',
        data:{'cat_id':$("#cat_list").val()},
        success: function (response) {
                if(response.success=='true'){
                     $("#advert_status_filter").css("display","block");
                     $("#div22").css("display","block");
                     $("#cat_list1").html("");
                     $("#cat_list1").append("<option value=''>Select Category Level 1</option>");

                     if (response.category1_list.length > 0) {
                         $("#cat_list1").append("<option value='all'>All</option>");
                   }
                    $.each(response.category1_list, function (index, item) {
                      data = '<option value="'+ item.category_id +'">'+ item.category_nm +'</option>'
                    $("#cat_list1").append(data);
            });
            }
        else {			$("#advert_status_filter").css("display","block");
                     $("#div22").css("display","none");
                     $("#cat_list1").html("");
                     $("#cat_list1").append("<option value=''>Select Category Level 1</option>");
            }

        },
        error: function (response) {
            alert("Error!");
        },

    });
}


