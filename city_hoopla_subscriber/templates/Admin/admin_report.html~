        $('#cat_list4').val("").change();

        $("#cat_list5").html("");
        $("#cat_list5").append("<option value=''>Select Category Level 5</option>");
        $('#cat_list5').val("").change();

        $("#advert_health_filter").css("display","none");

         $("#div55").css("display","none");
         $("#div66").css("display","none");
         $("#advert_status_filter").css("display","none");

    if($("#cat_list3").val() == ""){

        return false;
    }
    $.ajax({
        type: 'GET',
        url: '/get-catlevel4-list/',
        data:{'cat_id':$("#cat_list3").val()},
        success: function (response) {

          if(response.success=='true'){
               $("#div55").css("display","block");
            $("#cat_list4").html("");
            $("#cat_list4").append("<option value=''>Select Category Level 4</option>");

                 if (response.category4_list.length > 0) {
                 $("#cat_list4").append("<option value='all'>All</option>");
              }

            $.each(response.category4_list, function (index, item) {
                data = '<option value="'+ item.category_id +'">'+ item.category_nm +'</option>'
                $("#cat_list4").append(data);
            });

             }
         else {
            $("#advert_status_filter").css("display","block");
            $("#div55").css("display","none");
            $("#cat_list4").html("");
            $("#cat_list4").append("<option value=''>Select Category Level 4</option>");

             }

        },
        error: function (response) {
            alert("Error!");
        },

    });

}



    function get_advert_data(){
        $("#advert_list").html("");
        $("#advert_list").append("<option value=''>Select Advert</option>");
        $('#advert_list').val("").change();
        $("#advert_health_filter").css("display","none");
        if ($('#city_list').val()!=''  && $('#subscriber_list').val()!='') {
            $.ajax({
                type: 'GET',
                url: '/get-advert-data/',
                data:{
                    'city_id':$('#city_list').val(),
                    'subscriber_id':$('#subscriber_list').val()
                },
                success: function (response) {
                    $("#advert_list").html("");
                    $("#advert_list").append("<option value=''>Select Advert</option>");
                    if (response.advert_list.length > 0) {
                         $("#advert_list").append("<option value='all'>All</option>");
                         $("#advert_health_filter").css("display","none");
                    }
                    $.each(response.advert_list, function (index, item) {
                        data = '<option value="'+ item.advert_obj_id +'">'+ item.advert_obj_name +'</option>'
                        $("#advert_list").append(data);
                    });
                },
                error: function (response) {
                    alert("Error!");
                },

            });
        }
    }


function get_cat5_data(){

        $("#cat_list5").html("");
        $("#cat_list5").append("<option value=''>Select Category Level 5</option>");
        $('#cat_list5').val("").change();

         $("#div66").css("display","none");
         $("#advert_status_filter").css("display","none");

    if($("#cat_list4").val() == ""){

        return false;
    }
    $.ajax({
        type: 'GET',
        url: '/get-catlevel5-list/',
        data:{'cat_id':$("#cat_list4").val()},
        success: function (response) {
          if(response.success=='true'){
            $("#div66").css("display","block");
            $("#cat_list5").html("");
            $("#cat_list5").append("<option value=''>Select Category Level 5</option>");

                 if (response.category5_list.length > 0) {
                 $("#cat_list5").append("<option value='all'>All</option>");
              }


            $.each(response.category5_list, function (index, item) {
                data = '<option value="'+ item.category_id +'">'+ item.category_nm +'</option>'
                $("#cat_list5").append(data);
            });

             }
         else {
         	$("#advert_status_filter").css("display","block");
            $("#div66").css("display","none");
            $("#cat_list5").html("");
            $("#cat_list5").append("<option value=''>Select Category Level 5</option>");
             }



        },
        error: function (response) {
            alert("Error!");
        },

    });

     if($("#cat_list5").val() == ""){
      cat5_data();

     }

}

// Fourth  function
  function cat5_data(){

    if ($('#cat_list4').val()!=''  && $('#cat_list3').val()!=''  && $('#cat_list2').val()!=''  && $('#cat_list1').val()!=''  && $('#cat_list').val()!=''  ) {

            $.ajax({
                type: 'GET',
                url: '/get_advert_list5/',
                data:{'cat_id':$("#cat_list").val(),'cat1_id':$("#cat_list1").val(),'cat2_id':$("#cat_list2").val(),'cat3_id':$("#cat_list3").val(),'cat4_id':$("#cat_list4").val()},
                success: function (response) {
                    $("#advert_list").html("");
                    $("#advert_list").append("<option value=''>Select Advert</option>");

                     if (response.advert_list.length > 0) {
                         $("#advert_list").append("<option value='all'>All</option>");
                   }


                    $.each(response.advert_list, function (index, item) {
                        data = '<option value="'+ item.advert_obj_id +'">'+ item.advert_obj_name +'</option>'
                        $("#advert_list").append(data);
                    });
                },
                error: function (response) {
                    alert("Error!");
                },

        });
    }
   }


// Fifth  function
  function get_cat6_data(){

    if ($('#cat_list5').val()!=''  &&  $('#cat_list4').val()!=''  && $('#cat_list3').val()!=''  && $('#cat_list2').val()!=''  && $('#cat_list1').val()!=''  && $('#cat_list').val()!='' ) {

               $.ajax({
                type: 'GET',
                url: '/get_advert_list6/',
                data:{'cat_id':$("#cat_list").val(),'cat1_id':$("#cat_list1").val(),'cat2_id':$("#cat_list2").val(),'cat3_id':$("#cat_list3").val(),'cat4_id':$("#cat_list4").val(),'cat5_id':$("#cat_list5").val()},
                success: function (response) {
                    $("#advert_list").html("");
                    $("#advert_list").append("<option value=''>Select Advert</option>");


                     if (response.advert_list.length > 0) {
                         $("#advert_list").append("<option value='all'>All</option>");
                   }

                    $.each(response.advert_list, function (index, item) {
                        data = '<option value="'+ item.advert_obj_id +'">'+ item.advert_obj_name +'</option>'
                        $("#advert_list").append(data);
                    });
                },
                error: function (response) {
                    alert("Error!");
                },

        });
    }

    }

function get_advert_date(){
    if($("#advert_list").val() == ""){
       // $("#from_date").val("");
       // $("#to_date").val("");
        return false;
    }
    $.ajax({
        type: 'GET',
        url: '/get_advert_date/',
        data:{'advert_id':$("#advert_list").val()},
        success: function (response) {
            $("#from_date").val(response.start_date);
            $("#to_date").val(response.present_date);
            setTimeout(function(){
                from_date = $("#from_date").val();
                end_date = $("#to_date").val();
                advert_id = $("#advert_list").val();
                sub_id = $('#subscriber_list').val()

                data = '/get_advert_health/?advert_id='+advert_id+'&from_date='+from_date+'&to_date='+end_date+'&sub_id='+sub_id
                $('#advert_table').removeAttr('width').dataTable({
                    "bDestroy" : true,
                    buttons: [
                        //{ extend: 'print', className: 'btn dark btn-outline' },
                        { extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                            filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report",
                            customize: function(doc) {
                        doc.defaultStyle.fontSize = 8; //<-- set fontsize to 16 instead of 10
                        doc.defaultStyle.alignment= 'center'; //<-- set fontsize to 16 instead of 10
                        doc.styles.tableHeader.fontSize = 8; //<-- set fontsize to 16 instead of 10
                        }
                        },
                    ],

                    "order": [
                        [0, 'asc']
                    ],

                    "ajax": data,
                    "columns": [
                {"data": "advert_id","sClass": "text-center"},
                {"data": "advert_title"},
                {"data": "advert_views","sClass": "text-center"},
                {"data": "advert_likes","sClass": "text-center"},
                {"data": "advert_favourites","sClass": "text-center"},
                {"data": "advert_calls","sClass": "text-center"},
                {"data": "advert_call_backs","sClass": "text-center"},
                {"data": "advert_emails","sClass": "text-center"},
                {"data": "advert_coupons","sClass": "text-center"},
                {"data": "advert_reviews","sClass": "text-center"},
                {"data": "advert_sms","sClass": "text-center"},
                {"data": "advert_whatsapp","sClass": "text-center"},
                {"data": "advert_facebook","sClass": "text-center"},
                {"data": "advert_twitter","sClass": "text-center"}
            ],

                    "columnDefs": [
                {"className": "dt-center", "targets": "_all", width: 150}
            ],

             "paging":   true,

             "scrollX":     true,
                 "filter": false,
             "info":     false,
          // removeAttr('width').
                    "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable

                });
            }, 1000);
        },
        error: function (response) {
            alert("Error!");
        },

    });

}

function date_change(){

    if($("#advert_list").val() != '' && $("#from_date").val() !='' && $("#to_date").val() !=''){
        from_date = $("#from_date").val();
        end_date = $("#to_date").val();
        advert_id = $("#advert_list").val();
        data = '/get_advert_health/?advert_id='+advert_id+'&from_date='+from_date+'&to_date='+end_date
        $('#advert_table').removeAttr('width').dataTable({
            "bDestroy" : true,
            buttons: [
                //{ extend: 'print', className: 'btn dark btn-outline',
                //filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report" },
                { extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                    filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report",
                    customize: function(doc) {
                        doc.defaultStyle.fontSize = 8; //<-- set fontsize to 16 instead of 10
                        doc.defaultStyle.alignment= 'center'; //<-- set fontsize to 16 instead of 10
                        doc.styles.tableHeader.fontSize = 8; //<-- set fontsize to 16 instead of 10
                    }
                },
            ],

            "order": [
                [0, 'asc']
            ],

            "ajax": data,
             "columns": [
                {"data": "advert_id","sClass": "text-center"},
                {"data": "advert_title"},
                {"data": "advert_views","sClass": "text-center"},
                {"data": "advert_likes","sClass": "text-center"},
                {"data": "advert_favourites","sClass": "text-center"},
                {"data": "advert_calls","sClass": "text-center"},
                {"data": "advert_call_backs","sClass": "text-center"},
                {"data": "advert_emails","sClass": "text-center"},
                {"data": "advert_coupons","sClass": "text-center"},
                {"data": "advert_reviews","sClass": "text-center"},
                {"data": "advert_sms","sClass": "text-center"},
                {"data": "advert_whatsapp","sClass": "text-center"},
                {"data": "advert_facebook","sClass": "text-center"},
                {"data": "advert_twitter","sClass": "text-center"}
            ],

                   "columnDefs": [
                {"className": "dt-center", "targets": "_all", width: 150}
            ],

             "paging":   true,

             "scrollX":     true,
                 "filter": false,
             "info":     false,
          // removeAttr('width').
            "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable

        });
    }
    else if ($("#from_date").val() !='' & $("#to_date").val() !='') {

        from_date = $("#from_date").val();
        end_date = $("#to_date").val();

        data = '/get_advert_health_datebase/?from_date='+from_date+'&to_date='+end_date
        $('#advert_table').removeAttr('width').dataTable({
            "bDestroy" : true,
            buttons: [
                //{ extend: 'print', className: 'btn dark btn-outline',
               // filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report" },
                { extend: 'pdf', className: 'btn green btn-outline', orientation: 'landscape',
                    filename: 'Advert Health Report', "title": "CityHoopla : Advert Health Report",
                    customize: function(doc) {
                        doc.defaultStyle.fontSize = 8; //<-- set fontsize to 16 instead of 10
                        doc.defaultStyle.alignment= 'center'; //<-- set fontsize to 16 instead of 10
                        doc.styles.tableHeader.fontSize = 8; //<-- set fontsize to 16 instead of 10
                    }
                },
            ],

            "order": [
                [0, 'asc']
            ],

            "ajax": data,
            "columns": [
                {"data": "advert_id","sClass": "text-center"},
                {"data": "advert_title"},
                {"data": "advert_views","sClass": "text-center"},
                {"data": "advert_likes","sClass": "text-center"},
                {"data": "advert_favourites","sClass": "text-center"},
                {"data": "advert_calls","sClass": "text-center"},
                {"data": "advert_call_backs","sClass": "text-center"},
                {"data": "advert_emails","sClass": "text-center"},
                {"data": "advert_coupons","sClass": "text-center"},
                {"data": "advert_reviews","sClass": "text-center"},
                {"data": "advert_sms","sClass": "text-center"},
                {"data": "advert_whatsapp","sClass": "text-center"},
                {"data": "advert_facebook","sClass": "text-center"},
                {"data": "advert_twitter","sClass": "text-center"}
            ],

                "columnDefs": [
                {"className": "dt-center", "targets": "_all", width: 150}
            ],

             "paging":   true,

             "scrollX":     true,
                 "filter": false,
             "info":     false,
          // removeAttr('width').
            "dom": "<'row' <'col-md-12'B>><'row'<'col-md-6 col-sm-12'l><'col-md-6 col-sm-12'f>r><'table-scrollable't><'row'<'col-md-5 col-sm-12'i><'col-md-7 col-sm-12'p>>", // horizobtal scrollable datatable

        });

    }

    else{
        return false;
    }
}

function show_display_subscription(){
    if($('#subscribers_list').val() == ""){
        $("#display_subscription").css("display","none");
    }
    else{
        $("#display_subscription").css("display","block");
    }
}


function show_advert_health(){
    if($('#advert_list').val() == ""){
        $("#advert_health_filter").css("display","none");
    }
    else{
        $("#advert_health_filter").css("display","block");
    }
}


function get_category_data(){
        $("#display_subscription").css("display","none");
        $("#subscribers_list").html("");
        $("#subscribers_list").append("<option value=''>Select Subscriber</option>");
        $('#subscribers_list').val("").change();
        $("#sub_cat_list").html("");
        $("#sub_cat_list").append("<option value=''>Select Category</option>");
        $('#sub_cat_list').val("").change();
        $("#sub_cat_list1").html("");
        $("#sub_cat_list1").append("<option value=''>Select Category Level1</option>");
        $('#sub_cat_list1').val("").change();
        $("#sub_cat_list2").html("");
        $("#sub_cat_list2").append("<option value=''>Select Category Level2</option>");
        $('#sub_cat_list2').val("").change();
        $("#sub_cat_list3").html("");
        $("#sub_cat_list3").append("<option value=''>Select Category Level3</option>");
        $('#sub_cat_list3').val("").change();
        $("#sub_cat_list4").html("");
        $("#sub_cat_list4").append("<option value=''>Select Category Level4</option>");
        $('#sub_cat_list4').val("").change();

        $("#sub_cat_list5").html("");
        $("#sub_cat_list5").append("<option value=''>Select Category Level5</option>");


         $("#category_id1").css("display","none");
         $("#category_id2").css("display","none");
         $("#category_id3").css("display","none");
         $("#category_id4").css("display","none");
         $("#category_id5").css("display","none");
         $("#category_id6").css("display","none");

        if ($("#sub_city_list").val() =='') {
            return false;
        }
         $.ajax({
            type: 'GET',
            url: '/get-subscriber-list/',
            data:{'city_id':$("#sub_city_list").val()},
            success: function (response) {

                $("#subscribers_list").html("");

                 $("#subscribers_list").append("<option value=''>Select Subscriber</option>");
                if (response.supplier_list.length>0) {
                    $("#subscribers_list").append("<option value='all'>All</option>");
                }

                $.each(response.supplier_list, function (index, item) {
                    data = '<option value="'+ item.supplier_obj_id +'">'+ item.supplier_obj_name +'</option>'
                    $("#subscribers_list").append(data);
                });

            },
            error: function (response) {
                alert("Error!");
            },

        });
}

function get_cat_sub_data(){
            $("#display_subscription").css("display","none");
        $("#sub_cat_list").html("");
        $("#sub_cat_list").append("<option value=''>Select category</option>");
         $('#sub_cat_list').val("").change();
        $("#sub_cat_list1").html("");
        $("#sub_cat_list1").append("<option value=''>Select Category Level1</option>");
        $('#sub_cat_list1').val("").change();
        $("#sub_cat_list2").html("");
        $("#sub_cat_list2").append("<option value=''>Select Category Level2</option>");
        $('#sub_cat_list2').val("").change();
        $("#sub_cat_list3").html("");
        $("#sub_cat_list3").append("<option value=''>Select Category Level3</option>");
        $('#sub_cat_list3').val("").change();
        $("#sub_cat_list4").html("");
        $("#sub_cat_list4").append("<option value=''>Select Category Level4</option>");
        $('#sub_cat_list4').val("").change();

        $("#sub_cat_list5").html("");
        $("#sub_cat_list5").append("<option value=''>Select Category Level5</option>");


         $("#category_id1").css("display","none");
         $("#category_id2").css("display","none");
         $("#category_id3").css("display","none");
         $("#category_id4").css("display","none");
         $("#category_id5").css("display","none");
         $("#category_id6").css("display","none");


       if ($("#subscribers_list").val() =='') {
            return false;
          }

                $.ajax({
        type: 'GET',
        url: '/get-category1-list/',
        data:{'subscriber_id':$("#subscribers_list").val()},
        success: function (response) {

            $("#sub_cat_list").html("");
            $("#sub_cat_list").append("<option value=''>Select Category</option>");

                if (response.category_list.length>0) {
                    $("#sub_cat_list").append("<option value='all'>All</option>");
                }

            $.each(response.category_list, function (index, item) {
                data = '<option value="'+ item.category_id +'">'+ item.category_nm +'</option>'
                $("#sub_cat_list").append(data);
            });
        },
        error: function (response) {
            alert("Error!");
        },
    });
    }
