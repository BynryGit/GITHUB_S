        "scrollX": true
});

function get_subscriber_data(){

        $("#subscriber_list").html("");
        $("#subscriber_list").append("<option value=''>Select Subscriber</option>");
        $('#subscriber_list').val("").change();

        $("#advert_list").html("");
        $("#advert_list").append("<option value=''>Select Advert</option>");
        $('#advert_list').val("").change();
        $("#advert_health_filter").css("display","none");

        $('#sub_err').css("display", "none");
          $("#div1").css("display","none");
         $("#div2").css("display","none");
         $("#div3").css("display","none");
         $("#div4").css("display","none");
         $("#div5").css("display","none");
         $("#div6").css("display","none");

       if ($("#city_list").val() == '') {
        return false;
       }

   $.ajax({
        type: 'GET',
        url: '/get-subscriber-list/',
        data:{'city_id':$("#city_list").val()},
        success: function (response) {
            $("#subscriber_list").html("");
            $("#subscriber_list").append("<option value=''>Select Subscriber</option>");

            if (response.supplier_list.length > 0) {
                    $("#subscriber_list").append("<option value='all'>All</option>");

            }

            $.each(response.supplier_list, function (index, item) {
                data = '<option value="'+ item.supplier_obj_id +'">'+ item.supplier_obj_name +'</option>'
                $("#subscriber_list").append(data);
            });

        },
        error: function (response) {
            alert("Error!");
        },

    });

}

